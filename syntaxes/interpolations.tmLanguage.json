{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "scopeName": "interpolation.bloblang.injection",
  "injectionSelector": "L:string.quoted.single.yaml,L:string.quoted.double.yaml,L:string.unquoted.block.yaml",
  "patterns": [
    {
      "begin": "\\$\\{!",
      "end": "\\}",
      "name": "meta.interpolation.bloblang",
      "patterns": [{ "include": "source.bloblang#expressions" }]
    }
  ]
}
